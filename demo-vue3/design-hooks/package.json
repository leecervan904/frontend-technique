{
  "name": "@vue3/design-hooks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "start": "tsx ./server/main",
    "start:dev": "tsx watch ./server/main",
    "postinstall": "prisma generate",
    "dev:db:push": "dotenv -e ./env/.env.local -- npx prisma db push",
    "prod:db:push": "dotenv -e .env/.env.production.local -- npx prisma db push"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.54.6",
    "@prisma/client": "5.11.0",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "express": "^4.18.3",
    "prisma": "^5.11.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.27",
    "@vitejs/plugin-vue": "^5.0.4",
    "less": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vue-tsc": "^1.8.27"
  }
}